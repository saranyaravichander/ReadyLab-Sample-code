{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appService_function-sartest4_name": {
      "type": "string"
    },
    "appServicePlan_apphostplan-sartest3_location": {
      "type": "string"
    },
    "appServicePlan_apphostplan-sartest3_name": {
      "type": "string"
    },
    "appServicePlan_apphostplan-sartest3_resourceGroup": {
      "type": "string"
    },
    "storageaccount_storagesartest3": {
      "type": "string"
    }
  },
  "resources": [
    {
      "location": "[parameters('appServicePlan_apphostplan-sartest3_location')]",
      "name": "[parameters('appService_function-sartest4_name')]",
      "type": "Microsoft.Web/sites",
      "apiVersion": "2015-08-01",
      "tags": {
        "[concat('hidden-related:', resourceId(parameters('appServicePlan_apphostplan-sartest3_resourceGroup'),'Microsoft.Web/serverfarms', parameters('appServicePlan_apphostplan-sartest3_name')))]": "empty"
      },
      "kind": "functionapp",
      "properties": {
        "name": "[parameters('appService_function-sartest4_name')]",
        "serverFarmId": "[resourceId(parameters('appServicePlan_apphostplan-sartest3_resourceGroup'),'Microsoft.Web/serverfarms', parameters('appServicePlan_apphostplan-sartest3_name'))]",
        "kind": "functionapp",
        "httpsOnly": true,
        "siteConfig": {
          "alwaysOn": true
        }
      },
      "resources": [
        {
          "name": "appsettings",
          "type": "config",
          "apiVersion": "2015-08-01",
          "dependsOn": [
            "[concat('Microsoft.Web/Sites/', parameters('appService_function-sartest4_name'))]"
          ],
          "properties": {
            "FUNCTIONS_EXTENSION_VERSION": "~1",
            "AzureWebJobsDashboard": "[concat('DefaultEndpointsProtocol=https;AccountName=', parameters('storageaccount_storagesartest3'), ';AccountKey=', listKeys(resourceId('Quickstarts','Microsoft.Storage/storageAccounts', parameters('storageaccount_storagesartest3')), '2015-06-15').key1)]",
            "AzureWebJobsStorage": "[concat('DefaultEndpointsProtocol=https;AccountName=', parameters('storageaccount_storagesartest3'), ';AccountKey=', listKeys(resourceId('Quickstarts','Microsoft.Storage/storageAccounts', parameters('storageaccount_storagesartest3')), '2015-06-15').key1)]"
          }
        }
      ]
    }
  ]
}